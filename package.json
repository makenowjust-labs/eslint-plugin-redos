{
  "name": "eslint-plugin-redos",
  "description": "ESLint plugin for catching ReDoS vulnerability.",
  "license": "MIT",
  "author": "TSUYUSATO Kitsune <make.just.on@gmail.com>",
  "version": "1.1.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "prettier --list-different *.{json,md} src/**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@makenowjust-labo/redos": "^1.1.1",
    "inflected": "^2.1.0"
  },
  "peerDependencies": {
    "eslint": ">= 3"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.5",
    "@types/inflected": "^1.1.29",
    "@types/node": "16.7.10",
    "eslint": "^7.14.0",
    "jest": "27.1.0",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "27.0.5",
    "typescript": "^4.1.2"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "redos",
    "regexp",
    "security"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(src/.*\\.test)\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.d\\.ts$",
      "lib/.*"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
